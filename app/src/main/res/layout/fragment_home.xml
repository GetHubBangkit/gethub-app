<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nestedScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background"
        tools:context=".ui.home.HomeFragment">

        <ImageView
            android:id="@+id/frameGethub"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:src="@drawable/frame1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <!--    <ImageView-->
        <!--        android:id="@+id/homegethub"-->
        <!--        android:layout_width="145dp"-->
        <!--        android:layout_height="41dp"-->


        <!--        android:src="@drawable/homegethub"-->
        <!--        app:layout_constraintBottom_toBottomOf="@+id/frameGethub"-->
        <!--        app:layout_constraintEnd_toEndOf="parent"-->
        <!--        app:layout_constraintHorizontal_bias="0.06"-->
        <!--        app:layout_constraintStart_toStartOf="parent"-->
        <!--        app:layout_constraintTop_toTopOf="@+id/frameGethub" />-->

        <LinearLayout
            android:id="@+id/GethubPartnerTeks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="5dp"
            app:layout_constraintBottom_toBottomOf="@+id/frameGethub"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/frameGethub">

            <ImageView
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:layout_marginEnd="8dp"
                android:src="@drawable/icon_kelola">

            </ImageView>

            <TextView
                style="@style/Poppins.Black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Get"
                android:textColor="@color/blackgethub"
                android:textSize="24sp" />

            <TextView
                style="@style/Poppins.Black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:text="Hub"
                android:textColor="@color/teal"
                android:textSize="24sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/ivKelolaMyGethub"
            android:layout_width="80dp"
            android:layout_height="72dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/homekelolamygethub"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameGethub" />

        <TextView
            android:id="@+id/KelolaMyGethub"
            style="@style/Nunito"
            android:layout_width="80dp"
            android:layout_height="32dp"
            android:gravity="center"
            android:text="Kelola\nMy Gethub"
            android:textColor="@android:color/black"
            android:textSize="10sp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ivKelolaMyGethub" />

        <ImageView
            android:id="@+id/ivCariProjectBid"
            android:layout_width="80dp"
            android:layout_height="72dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/homecariprojectbid"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.077"
            app:layout_constraintStart_toEndOf="@id/ivKelolaMyGethub"
            app:layout_constraintTop_toBottomOf="@+id/frameGethub"
            app:layout_constraintVertical_bias="0.015" />

        <TextView
            android:id="@+id/CariProjectBids"
            style="@style/Nunito"
            android:layout_width="80dp"

            android:layout_height="32dp"
            android:gravity="center"
            android:text="Cari\nProject Bids"
            android:textColor="@android:color/black"
            android:textSize="10sp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.076"
            app:layout_constraintStart_toEndOf="@+id/KelolaMyGethub"
            app:layout_constraintTop_toBottomOf="@+id/ivCariProjectBid" />

        <ImageView
            android:id="@+id/ivCariTalent"
            android:layout_width="80dp"
            android:layout_height="72dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/homecaritalent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.127"
            app:layout_constraintStart_toEndOf="@+id/ivCariProjectBid"
            app:layout_constraintTop_toBottomOf="@+id/frameGethub"
            app:layout_constraintVertical_bias="0.015" />

        <TextView
            android:id="@+id/CariTalent"
            style="@style/Nunito"
            android:layout_width="80dp"

            android:layout_height="32dp"
            android:gravity="center"
            android:text="Cari\nTalent"
            android:textColor="@android:color/black"
            android:textSize="10sp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.13"
            app:layout_constraintStart_toEndOf="@+id/CariProjectBids"
            app:layout_constraintTop_toBottomOf="@+id/ivCariTalent" />


        <ImageView
            android:id="@+id/ivProjectDetector"
            android:layout_width="80dp"
            android:layout_height="72dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/homeprojectdetector"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ivCariTalent"
            app:layout_constraintTop_toBottomOf="@+id/frameGethub" />

        <TextView
            android:id="@+id/ProjectDetector"
            style="@style/Nunito"
            android:layout_width="80dp"

            android:layout_height="32dp"
            android:gravity="center"
            android:text="Project\nDetector"
            android:textColor="@color/black"
            android:textSize="10sp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.459"
            app:layout_constraintStart_toEndOf="@+id/CariTalent"
            app:layout_constraintTop_toBottomOf="@+id/ivProjectDetector" />

        <LinearLayout
            android:id="@+id/HomeGethubPartner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/KelolaMyGethub">

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GetHub"
                android:textColor="@color/teal"
                android:textSize="16sp" />

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:text="Partner"
                android:textColor="@color/blackgethub"
                android:textSize="16sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/homenew"
            android:layout_width="52dp"
            android:layout_height="18dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/homenew"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.079"
            app:layout_constraintStart_toEndOf="@+id/HomeGethubPartner"
            app:layout_constraintTop_toBottomOf="@+id/KelolaMyGethub" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewHomeGethubPartner"
            android:layout_width="match_parent"
            android:layout_height="99dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/HomeGethubPartner" />


        <ProgressBar
            android:id="@+id/progressBarOnNewGethubPartner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/recyclerViewHomeGethubPartner"
            app:layout_constraintEnd_toEndOf="@+id/recyclerViewHomeGethubPartner"
            app:layout_constraintStart_toStartOf="@+id/recyclerViewHomeGethubPartner"
            app:layout_constraintTop_toTopOf="@+id/recyclerViewHomeGethubPartner"

            tools:visibility="visible" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clEmptyGethubPartner"
            android:layout_width="match_parent"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_height="66dp"
            app:layout_constraintBottom_toBottomOf="@+id/recyclerViewHomeGethubPartner"
            app:layout_constraintTop_toTopOf="@+id/recyclerViewHomeGethubPartner">

            <ImageView
                android:id="@+id/ivEmptyGethubPartner"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/frame1"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ivEmptyImageGethubPartner"
                android:layout_width="49dp"
                android:layout_height="38dp"
                android:src="@drawable/emptyimage"
                app:layout_constraintBottom_toBottomOf="@id/ivEmptyGethubPartner"
                app:layout_constraintStart_toStartOf="@id/ivEmptyGethubPartner"
                app:layout_constraintTop_toTopOf="@id/ivEmptyGethubPartner" />

            <TextView
                android:id="@+id/tvEmptyGethubPartner"
                style="@style/Nunito"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="Belum ada Gethub Partner Baru"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="@id/ivEmptyGethubPartner"
                app:layout_constraintStart_toEndOf="@+id/ivEmptyImageGethubPartner"
                app:layout_constraintTop_toTopOf="@id/ivEmptyGethubPartner"
                app:layout_constraintEnd_toEndOf="@id/ivEmptyGethubPartner"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/homebiddingdikerjakan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/recyclerViewHomeGethubPartner">

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bidding"
                android:textColor="@color/teal"
                android:textSize="16sp" />

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:text="Dikerjakan"
                android:textColor="@color/blackgethub"
                android:textSize="16sp" />
        </LinearLayout>


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewBiddingDikerjakan"
            android:layout_width="match_parent"
            android:layout_height="119dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/homebiddingdikerjakan"
            tools:listitem="@layout/item_home_biddingdikerjakan" />

        <ProgressBar
            android:id="@+id/progressBarOnBiddingDikerjakan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@id/recyclerViewBiddingDikerjakan"
            app:layout_constraintEnd_toEndOf="@id/recyclerViewBiddingDikerjakan"
            app:layout_constraintStart_toStartOf="@id/recyclerViewBiddingDikerjakan"
            app:layout_constraintTop_toTopOf="@id/recyclerViewBiddingDikerjakan"
            tools:visibility="visible" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clEmptyBiddingDikerjakan"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/recyclerViewBiddingDikerjakan"
            app:layout_constraintTop_toTopOf="@+id/recyclerViewBiddingDikerjakan">

            <ImageView
                android:id="@+id/ivEmptyBiddingDikerjakan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/frame1"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ivEmptyImageBiddingDikerjakan"
                android:layout_width="49dp"
                android:layout_height="38dp"
                android:src="@drawable/emptyimage"
                app:layout_constraintBottom_toBottomOf="@id/ivEmptyBiddingDikerjakan"
                app:layout_constraintStart_toStartOf="@id/ivEmptyBiddingDikerjakan"
                app:layout_constraintTop_toTopOf="@id/ivEmptyBiddingDikerjakan" />

            <TextView
                android:id="@+id/tvEmptyBiddingDikerjakan"
                style="@style/Nunito"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="Belum ada bidding pekerjaan yang kamu kerjakan"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="@id/ivEmptyBiddingDikerjakan"
                app:layout_constraintStart_toEndOf="@+id/ivEmptyImageBiddingDikerjakan"
                app:layout_constraintTop_toTopOf="@id/ivEmptyBiddingDikerjakan"
                app:layout_constraintEnd_toEndOf="@id/ivEmptyBiddingDikerjakan"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/homeinformationhub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/recyclerViewBiddingDikerjakan">

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rekomendasi"
                android:textColor="@color/teal"
                android:textSize="16sp" />

            <TextView
                style="@style/Poppins.Bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:text="Event"
                android:textColor="@color/blackgethub"
                android:textSize="16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/tvSeeAllInformation"
            android:layout_marginTop="19dp"
            style="@style/Poppins.SemiBold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="8dp"
            android:text="Lihat Semua"
            android:textColor="@color/teal"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"


            app:layout_constraintTop_toBottomOf="@+id/recyclerViewBiddingDikerjakan" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewInformationHub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="50dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/homeinformationhub" />

        <ProgressBar
            android:id="@+id/progressBarOnInformationHub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/recyclerViewInformationHub"
            app:layout_constraintEnd_toEndOf="@+id/recyclerViewInformationHub"
            app:layout_constraintStart_toStartOf="@+id/recyclerViewInformationHub"
            app:layout_constraintTop_toTopOf="@+id/recyclerViewInformationHub"

            tools:visibility="visible" />



        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clEmptyEvent"
            android:layout_width="match_parent"
            android:layout_height="242dp"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/recyclerViewInformationHub"
            app:layout_constraintTop_toTopOf="@+id/recyclerViewInformationHub">

            <ImageView
                android:id="@+id/ivEmptyEvent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/frame1"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="@+id/clEmptyEvent"
                app:layout_constraintStart_toStartOf="@+id/clEmptyEvent"
                app:layout_constraintTop_toTopOf="@+id/clEmptyEvent"
                app:layout_constraintBottom_toBottomOf="@+id/clEmptyEvent"/>

            <ImageView
                android:id="@+id/ivEmptyImageEvent"
                android:layout_width="312dp"
                android:layout_height="217dp"
                android:src="@drawable/emptyimageevent"
                app:layout_constraintEnd_toEndOf="@+id/ivEmptyEvent"
                app:layout_constraintStart_toStartOf="@+id/ivEmptyEvent" />

            <TextView
                android:id="@+id/tvEmptyEvent"
                style="@style/Nunito"
                android:layout_width="0dp"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:text="Tidak Ada Event Rekomendasi yg cocok untuk kamu"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="@+id/clEmptyEvent"
                app:layout_constraintStart_toStartOf="@+id/clEmptyEvent"
                app:layout_constraintTop_toBottomOf="@+id/ivEmptyImageEvent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>