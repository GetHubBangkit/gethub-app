<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:fillViewport="true"
    tools:context=".ui.analitic.AnaliticFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background"
        tools:context=".ui.analitic.AnaliticFragment">

    <ImageView
        android:id="@+id/frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/rectangle"
        app:layout_constraintDimensionRatio="H,3:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textanalitic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Analitic"
        android:layout_marginTop="18dp"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        app:fontFamily="@font/poppins_bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frame"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivCardViewer"
        android:layout_width="110dp"
        android:layout_height="88dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/projectyellow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textanalitic"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:id="@+id/progressBarOnCardViewer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/ivCardViewer"
        app:layout_constraintEnd_toEndOf="@+id/ivCardViewer"
        app:layout_constraintStart_toStartOf="@+id/ivCardViewer"
        app:layout_constraintTop_toTopOf="@+id/ivCardViewer"

        tools:visibility="visible" />

    <TextView
        android:id="@+id/jumlah_cardviewer"
        android:layout_width="23dp"
        android:layout_height="42dp"
        android:gravity="center"
        android:fontFamily="@font/poppins_bold"
        android:text="11"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:fontFamily="@font/poppins_bold"
       android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/ivCardViewer"
        app:layout_constraintStart_toStartOf="@+id/ivCardViewer"
        app:layout_constraintTop_toTopOf="@+id/ivCardViewer"
    />

    <TextView
        android:id="@+id/tvCardViewers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:fontFamily="@font/poppins_bold"
        android:text="Card Viewer"
        android:layout_marginTop="3dp"
        android:textColor="@color/textproject"
        android:textSize="12sp"
        app:fontFamily="@font/nunito_bold"

        app:layout_constraintEnd_toEndOf="@+id/ivCardViewer"


        app:layout_constraintStart_toStartOf="@+id/ivCardViewer"
        app:layout_constraintTop_toBottomOf="@+id/jumlah_cardviewer"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivWebViewer"
        android:layout_width="110dp"
        android:layout_height="88dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/projectblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textanalitic"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:id="@+id/progressBarOnWebViewer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/ivWebViewer"
        app:layout_constraintEnd_toEndOf="@+id/ivWebViewer"
        app:layout_constraintStart_toStartOf="@+id/ivWebViewer"
        app:layout_constraintTop_toTopOf="@+id/ivWebViewer"
        tools:visibility="visible"/>


    <TextView
        android:id="@+id/jumlah_webviewer"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:fontFamily="@font/poppins_bold"
        android:text="13"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:fontFamily="@font/poppins_bold"

        app:layout_constraintEnd_toEndOf="@+id/ivWebViewer"
        app:layout_constraintStart_toStartOf="@+id/ivWebViewer"
        app:layout_constraintTop_toTopOf="@+id/ivWebViewer"
    />

    <TextView
        android:id="@+id/tvWebViewers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:gravity="center"
        android:fontFamily="@font/poppins_bold"
        android:text="Web Viewer"
        android:textColor="@color/textproject"
        android:textSize="12sp"
        app:fontFamily="@font/nunito_bold"

        app:layout_constraintEnd_toEndOf="@+id/ivWebViewer"



        app:layout_constraintStart_toStartOf="@+id/ivWebViewer"
        app:layout_constraintTop_toBottomOf="@+id/jumlah_webviewer"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivPartner"
        android:layout_width="110dp"
        android:layout_height="88dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="18dp"
        android:src="@drawable/projectgreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/frame"
        app:layout_constraintTop_toBottomOf="@+id/textanalitic"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:id="@+id/progressBarOnPartner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/ivPartner"
        app:layout_constraintEnd_toEndOf="@+id/ivPartner"
        app:layout_constraintStart_toStartOf="@+id/ivPartner"
        app:layout_constraintTop_toTopOf="@+id/ivPartner"
        tools:visibility="visible"/>
    <TextView
        android:id="@+id/jumlahpartner"
        android:layout_width="27dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="6"
        android:gravity="center"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:fontFamily="@font/poppins_bold"

        app:layout_constraintEnd_toEndOf="@+id/ivPartner"

        app:layout_constraintStart_toStartOf="@+id/ivPartner"
        app:layout_constraintTop_toTopOf="@+id/ivPartner"
   />

    <TextView
        android:id="@+id/tvPartner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:layout_marginTop="3dp"
        android:text="Partner"
        android:textColor="@color/textproject"
        android:textSize="12sp"
        app:fontFamily="@font/nunito_bold"

        app:layout_constraintEnd_toEndOf="@+id/ivPartner"


        app:layout_constraintStart_toStartOf="@+id/ivPartner"
        app:layout_constraintTop_toBottomOf="@+id/jumlahpartner"
     />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:fitsSystemWindows="true"
        android:orientation="vertical"
        android:layout_marginTop="20dp"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivWebViewer">

        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/lineChart"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="9dp"
            tools:layout_editor_absoluteX="-10dp"
            tools:layout_editor_absoluteY="155dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"/>

    </LinearLayout>

    <TextView
        android:id="@+id/gethubkamudilihatoleh"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Gethub Kamu Di Lihat Oleh"
        android:textColor="@color/blackgethub"
        android:textSize="20sp"
        app:fontFamily="@font/poppins_bold"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.106" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewGethubDilihat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gethubkamudilihatoleh"
        app:layout_constraintVertical_bias="0.1" />


    <ProgressBar
        android:id="@+id/progressBarOnGethubKamuDilihat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerViewGethubDilihat"
        app:layout_constraintEnd_toEndOf="@+id/recyclerViewGethubDilihat"
        app:layout_constraintStart_toStartOf="@+id/recyclerViewGethubDilihat"
        app:layout_constraintTop_toTopOf="@+id/recyclerViewGethubDilihat"

        tools:visibility="visible" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clEmptyGethubDilihat"
        android:layout_width="match_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_height="66dp"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerViewGethubDilihat"
        app:layout_constraintTop_toTopOf="@+id/recyclerViewGethubDilihat">

        <ImageView
            android:id="@+id/ivEmptyGethubDilihat"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/frame1"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ivEmptyImageGethubDilihat"
            android:layout_width="49dp"
            android:layout_height="38dp"
            android:src="@drawable/emptyimage"
            app:layout_constraintBottom_toBottomOf="@id/ivEmptyGethubDilihat"
            app:layout_constraintStart_toStartOf="@id/ivEmptyGethubDilihat"
            app:layout_constraintTop_toTopOf="@id/ivEmptyGethubDilihat" />

        <TextView
            android:id="@+id/tvEmptyGethubDilihat"
            style="@style/Nunito"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Yah! Belum ada Yang Melihat Gethub mu"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@id/ivEmptyGethubDilihat"
            app:layout_constraintStart_toEndOf="@+id/ivEmptyImageGethubDilihat"
            app:layout_constraintTop_toTopOf="@id/ivEmptyGethubDilihat"
            app:layout_constraintEnd_toEndOf="@id/ivEmptyGethubDilihat"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>